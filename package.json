{
	"name": "svelte-brick-gallery",
	"version": "0.0.5",
	"description": "A masonry-like image gallery component for svelte",
	"keywords": [
		"svelte",
		"svelte-component",
		"masonry",
		"brick",
		"image",
		"gallery"
	],
	"author": {
		"name": "anotherempty",
		"url": "https://github.com/anotherempty"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/anotherempty/svelte-brick-gallery.git"
	},
	"homepage": "https://anotherempty.github.io/svelte-brick-gallery/",
	"bugs": {
		"url": "https://github.com/anotherempty/svelte-brick-gallery/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte2tsx": "^0.5.9",
		"typescript": "~4.6.2"
	},
	"type": "module"
}
