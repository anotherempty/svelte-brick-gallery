{
	"name": "svelte-brick-gallery",
	"version": "1.0.1",
	"description": "A masonry-like image gallery component for svelte",
	"keywords": [
		"svelte",
		"svelte-component",
		"masonry",
		"brick",
		"image",
		"gallery"
	],
	"author": {
		"name": "anotherempty",
		"url": "https://github.com/anotherempty"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/anotherempty/svelte-brick-gallery.git"
	},
	"homepage": "https://anotherempty.github.io/svelte-brick-gallery/",
	"bugs": {
		"url": "https://github.com/anotherempty/svelte-brick-gallery/issues"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.9",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
